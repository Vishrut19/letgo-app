{
  "expo": {
    "name": "LetGo",
    "slug": "letgo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/letgo-logo.png",
    "backgroundColor": "#1C1E26",
    "scheme": "letgo",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.vishrutagarwalla.letgo",
      "supportsTablet": false,
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.vishrutagarwalla.letgo",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/letgo-logo.png",
        "backgroundColor": "#1C1E26"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1c1e26",
          "dark": {
            "backgroundColor": "#1c1e26"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "984a4dbb-e960-4b15-aa97-cc353fc8e368"
      }
    }
  }
}